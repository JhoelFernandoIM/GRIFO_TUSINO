   <string notr="true">/* Fondo de la ventana */
QMainWindow {
    background-color: #F4F4F4;
}

/* ==========================
   MENU BAR (barra superior)
   ========================== */
QMenuBar {
    background-color: #33373B;
    color: #FFFFFF;
    font-size: 14px;
    font-weight: bold;
}

QMenuBar::item {
    background: transparent;
    padding: 12px 24px;
}

QMenuBar::item:selected { /* Hover */
    background: #2ABF9E;
    color: #FFFFFF;
    border-radius: 5px;
}

QMenuBar::item:pressed {
    background: #1F8C72;
}

/* ==========================
   MENÚS DESPLEGABLES
   ========================== */
QMenu {
    background-color: #33373B;
    color: #FFFFFF;
    border: 1px solid #2ABF9E;
    padding: 5px;
}

QMenu::item {
    padding: 6px 20px;
    background: transparent;
}

QMenu::item:selected { /* Hover */
    background: #2ABF9E;
    border-radius: 5px;
}

/* ==========================
   TOOLBAR (acciones con icono/botón)
   ========================== */
QToolBar {
    background: #33373B;
    border: none;
    spacing: 6px;
    padding: 4px;
}

/* Los QAction en la toolbar aparecen como QToolButton */
QToolButton {
    background: transparent;
    color: #FFFFFF;
    padding: 8px 12px;
    border-radius: 8px;
    font-size: 14px;
    font-weight: bold;
}

QToolButton:hover {
    background: #2ABF9E;
    color: #FFFFFF;
}

QToolButton:pressed {
    background: #1F8C72;
}
